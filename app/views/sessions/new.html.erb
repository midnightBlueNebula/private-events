<h1>Log in</h1>

<%= form_for(:sessions, url: "/login") do |f| %>
    
    <%= f.label :email %> 
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.label :remember_me %>
    <%= f.check_box :remember_me %>

    <%= f.submit :log_in %>

<% end %>
