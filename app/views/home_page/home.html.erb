<h1>Private Events</h1>

<%= link_to 'Home', root_path %>

<% if logged_in? %>
    <%= link_to 'Users', users_path %>
    <%= link_to 'Profile', user_path(current_user) %>
    <%= link_to 'Log Out', logout_path, method: :delete %>
<% else %>
    <%= link_to 'Log In', login_path %>
    <%= link_to 'Sign Up', signup_path %>
<% end %>